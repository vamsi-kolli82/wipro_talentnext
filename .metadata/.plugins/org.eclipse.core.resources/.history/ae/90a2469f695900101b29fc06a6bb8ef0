import java.util.*;
public class Q_10 {

	public static void main(String[] args) {

		Scanner sc=new Scanner(System.in);
		char a=sc.nextLine().charAt(0);
		if(a>=65 && a<=90) {
			int b=a+65;
			System.out.println((char)b);
		}
		else {
			int b=a-65;
			System.out.println((char)b);
		}
		sc.close();
	}

}
